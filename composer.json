{
    "require": {
        "wp-api/client": "dev-master",
        "justinrainbow/json-schema": "1.4.*@dev",
		"slim/slim": "2.*",
        "gabordemooij/redbean": "3.5.1",
        "daviddeutsch/redbean-prefix": "dev-master",
        "raveren/kint": "0.9",
        "swiftmailer/swiftmailer": "5.0.3",
        "egulias/email-validator": "1.2.5",
        "flynsarmy/slim-monolog": "dev-master",
        "google/apiclient": "1.0.*@beta",
        "mailgun/mailgun-php": "1.7.2",
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "whatthejeff/nyancat-phpunit-resultprinter": "v1.2.0",
        "guzzle/guzzle": "~3.8",
        "mikey179/vfsStream": "1.2.*",
        "there4/slim-test-helpers": "dev-master",
        "fzaninotto/faker": "dev-master",
        "codeclimate/php-test-reporter": "dev-master",
        "satooshi/php-coveralls": "0.6.1",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "*"
    },
    "autoload": {
	    "psr-0": {
	        "": "vendor/wp-api/client/library"
	    },
        "psr-4": { 
            "Dreamland\\": "src/Dreamland/",
            "BitPrepared\\": "src/BitPrepared/"
        }
	},
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/yoghi/redbean-prefix.git"
        }
    ],
    "archive": {
        "exclude": ["vendor", ".DS_Store", "*.log"]
    }
}
